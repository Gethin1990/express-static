<section class="row centered mt grid">

    <div class="row center-block">
        <div class="col-md-4 col-md-offset-4 centered ">
            <hr>
            <h4>
                <span class="text-muted">Internet Access</span>
            </h4>
        </div>
    </div>

    <!-- Take Images from gallery-internetaccess.json for Gallery -->

        <% if (iaPic.length > 0) { %>
            <% for (i=1; i<iaPic.length; i++) { %>
                <div class="col-xs-6 col-lg-4 mb">
                    <a href="http://localhost:3000/<%= iaUrl[i] %>"><img src="<%= iaPic[i] %>" alt="<%= iaPicAlt[i] %>" class="img-fluid"></a>
                </div>
            <% } %>
        <% } %>

</section>
