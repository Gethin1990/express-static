<section class="row centered grid">

    <div class="row center-block">
        <div class="col-md-4 col-md-offset-4 centered ">
            <hr>
            <h4>
                <span class="text-muted">Motion Detection</span>
            </h4>
        </div>
    </div>

    <!-- Take Images from gallery-quickinstallation.json for Gallery -->

    <% if (mdPic.length > 0) { %>
        <% for (i=1; i<mdPic.length; i++) { %>
            <div class="col-xs-6 col-lg-4 mb">
                <a href="http://localhost:3000/<%= mdUrl[i] %>"><img src="<%= mdPic[i] %>" alt="<%= mdPicAlt[i] %>" class="img-fluid"></a>
            </div>
        <% } %>
    <% } %>

</section>
